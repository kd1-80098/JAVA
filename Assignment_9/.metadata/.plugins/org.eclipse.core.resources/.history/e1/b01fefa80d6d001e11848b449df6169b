
public class Book {
	
	private String isbn;
	private double price;
	private String authorName;
	private int quantity;
	public Book(String isbn, double price, String authorName, int quantity) {
		super();
		this.isbn = isbn;
		this.price = price;
		this.authorName = authorName;
		this.quantity = quantity;
	}
	public Book() {
	
	}
	
	
}
